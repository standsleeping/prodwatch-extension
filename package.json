{
  "name": "prodwatch",
  "displayName": "ProdWatch",
  "publisher": "ProdWatch",
  "description": "See function calls in production from your editor.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "prodwatch.login",
        "title": "ProdWatch: Login"
      }
    ],
    "configuration": {
      "title": "ProdWatch",
      "properties": {
        "prodwatch.apiUrl": {
          "type": "string",
          "default": "https://getprodwatch.com",
          "description": "Base URL for ProdWatch API (use http://localhost:8000 for local development)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "dependencies": {
    "vscode-languageclient": "^8.1.0"
  }
}
